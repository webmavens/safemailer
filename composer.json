{
    "name": "safemailer/safemailer",
    "description": "A Laravel package for secure email attachments with expiration",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SafeMailer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "SafeMailer",
            "email": "support@safemailer.io"
        }
    ],
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.334"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SafeMailer\\SafeMailerServiceProvider"
            ],
            "aliases": {
                "SafeMailer": "SafeMailer\\SafeMailerFacade"
            }
        },
        "files": [
        "config/safemailer.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 
